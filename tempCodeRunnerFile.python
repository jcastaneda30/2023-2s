import random

# Genera 5000 números aleatorios
numeros_aleatorios = [random.randint(1, 1e12) for _ in range(5000)]

# Guarda los números en un archivo
with open('numeros_aleatorios.txt', 'w') as archivo:
    for numero in numeros_aleatorios:
        archivo.write(f'1{numero}\n')

print("Archivo 'numeros_aleatorios.txt' creado con éxito.")
